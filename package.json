{
  "name": "qckfx",
  "version": "0.1.20",
  "description": "A web-based AI software engineering agent server",
  "main": "dist/server/index.js",
  "types": "dist/server/index.d.ts",
  "bin": {
    "qckfx": "dist/server/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test:server": "jest",
    "test:ui": "vitest run --config vitest.config.ts",
    "test:ui:watch": "vitest --config vitest.config.ts",
    "test:ui:coverage": "vitest run --config vitest.config.ts --coverage",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern 'src/ui/**'",
    "lint:ui": "eslint 'src/ui/**/*.{ts,tsx}'",
    "lint:all": "eslint src/**/*.{ts,tsx}",
    "build:server": "tsc && chmod +x dist/server/index.js",
    "build:ui": "vite build",
    "dev:ui": "vite",
    "dev:server": "ts-node src/server/index.ts --port 3002",
    "wait-on-server": "wait-on -t 30000 http-get://localhost:3002/health",
    "dev:ui:after-server": "npm run wait-on-server && npm run dev:ui",
    "dev": "concurrently -n \"tsc,server,ui\" \"npm run build:watch\" \"npm run dev:server\" \"npm run dev:ui:after-server\"",
    "build:watch": "tsc --watch",
    "build": "npm run build:server && npm run build:ui",
    "typecheck:server": "tsc -p tsconfig.server.json --noEmit",
    "typecheck:ui": "tsc -p tsconfig.ui.json --noEmit",
    "typecheck": "npm run typecheck:server && npm run typecheck:ui",
    "ui:generate": "npx shadcn-ui@latest add",
    "start:dist": "node dist/server/index.js",
    "start:dev": "npm run build && npm run start:dist -- --port 3000",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "framework",
    "tools",
    "composition",
    "claude",
    "anthropic"
  ],
  "author": "qckfx",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@monaco-editor/react": "^4.7.0",
    "@qckfx/agent": "0.2.1-alpha.15",
    "@qckfx/schema": "^0.0.3",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@types/body-parser": "^1.19.5",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/cors": "^2.8.17",
    "@types/diff": "^7.0.2",
    "@types/express": "^5.0.1",
    "@types/prompts": "^2.4.9",
    "@types/uuid": "^10.0.0",
    "body-parser": "^1.20.3",
    "commander": "^13.1.0",
    "connect-history-api-fallback": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "diff": "^7.0.0",
    "dotenv": "^16.4.5",
    "events": "^3.3.0",
    "express": "^4.21.2",
    "inversify": "^7.4.0",
    "monaco-editor": "^0.52.2",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.4.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jsdom": "^24.0.0",
    "lucide-react": "^0.359.0",
    "msw": "^2.2.3",
    "postcss": "^8.4.38",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^5.2.6",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^1.4.0",
    "wait-on": "^8.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
